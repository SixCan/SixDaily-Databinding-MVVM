<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ca.six.daily">

<uses-permission android:name="android.permission.INTERNET"/>

    <application android:name=".core.BaseApp" tools:replace="android:supportsRtl"
        android:label="@string/app_name" android:supportsRtl="false"
        android:icon="@drawable/ic_launcher" android:roundIcon="@drawable/ic_launcher_round"
        android:theme="@style/AppTheme">

        <activity android:name=".biz.aboutme.AboutMeActivity" />
        <activity android:name=".biz.home.DailyListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".biz.detail.DailyDetailActivity" />

    </application>


</manifest>